[ignore]
.*/node_modules/ava/.*
.*/lib/.*

[include]

[libs]
./flow-typed

[options]
module.name_mapper='^definitions\/\(.*\)$' -> '<PROJECT_ROOT>/definitions/\1'
module.name_mapper='^src\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'

module.system=haste

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable